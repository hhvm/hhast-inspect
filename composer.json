        "dev-master": "4.x-dev",
        "dev-main": "4.x-dev"
      }
    },
    "require": {
        "hhvm": "^4.128",
        "hhvm/hhast": "^4.78",
        "hhvm/hsl": "^4.41",
        "hhvm/hhvm-autoload": "^3.0",
        "facebook/hh-clilib": "^2.5.0rc1",
        "facebook/xhp-lib": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "hhvm/hhvm-autoload": true
        }
    }
}
